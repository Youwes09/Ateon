$barBg: color.adjust($background, $alpha: -0.5);
$surface: color.adjust($surface_bright, $alpha: -0.4);
$overlay: color.adjust($on_surface, $alpha: -0.25);
$accent: $tertiary_container;

/* os icon colors */
$os: $on_surface_variant;

/* workspaces colors */
$primaryMon: color.mix(
  $primary_container,
  $on_primary_container,
  70%,
  $method: rgb
);
$primaryMonHover: color.mix(
  $primary_container,
  $on_primary_container,
  45%,
  $method: rgb
);
$secondaryMon: color.mix(
  $tertiary_container,
  $on_tertiary_container,
