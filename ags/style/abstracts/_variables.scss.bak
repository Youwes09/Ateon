);
$fourthMon: color.mix($secondary, $on_secondary, 70%, $method: rgb);
$fourthMonHover: color.mix($secondary, $on_secondary, 45%, $method: rgb);

/* button colors */
$buttonEnabled: color.adjust($primaryMon, $alpha: -0.5);
$buttonEnabledHover: color.adjust($buttonEnabled, $lightness: -15%);

$buttonDisabled: $surface;
$buttonDisabledHover: color.adjust($buttonDisabled, $alpha: +0.8);

/* hw circular proc colors */
$ramProc: color.mix($primary_fixed_dim, $on_primary_fixed_variant, 66%);
