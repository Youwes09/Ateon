@use "sass:color";
@use "theme_variables_matugen";

/*
* Css Colors
* Generated with Matugen
*/


$background: rgba(19, 19, 21, 1.0);

$error: rgba(255, 180, 171, 1.0);

$error_container: rgba(147, 0, 10, 1.0);

$inverse_on_surface: rgba(49, 48, 50, 1.0);

$inverse_primary: rgba(91, 94, 108, 1.0);

$inverse_surface: rgba(229, 226, 227, 1.0);

$on_background: rgba(229, 226, 227, 1.0);

$on_error: rgba(105, 0, 5, 1.0);

$on_error_container: rgba(255, 218, 214, 1.0);

$on_primary: rgba(45, 48, 61, 1.0);

$on_primary_container: rgba(224, 225, 243, 1.0);

$on_primary_fixed: rgba(24, 27, 39, 1.0);

$on_primary_fixed_variant: rgba(68, 70, 84, 1.0);

$on_secondary: rgba(47, 48, 56, 1.0);

$on_secondary_container: rgba(226, 225, 236, 1.0);

$on_secondary_fixed: rgba(26, 27, 35, 1.0);

$on_secondary_fixed_variant: rgba(69, 70, 79, 1.0);

$on_surface: rgba(229, 226, 227, 1.0);

$on_surface_variant: rgba(200, 198, 199, 1.0);

$on_tertiary: rgba(44, 47, 66, 1.0);

$on_tertiary_container: rgba(222, 225, 249, 1.0);

$on_tertiary_fixed: rgba(23, 27, 44, 1.0);

$on_tertiary_fixed_variant: rgba(66, 70, 89, 1.0);

$outline: rgba(146, 144, 146, 1.0);

$outline_variant: rgba(71, 70, 72, 1.0);

$primary: rgba(196, 197, 214, 1.0);

$primary_container: #343434;

$primary_fixed: rgba(224, 225, 243, 1.0);

$primary_fixed_dim: rgba(196, 197, 214, 1.0);

$scrim: rgba(0, 0, 0, 1.0);

$secondary: rgba(198, 197, 208, 1.0);

$secondary_container: rgba(69, 70, 79, 1.0);

$secondary_fixed: rgba(226, 225, 236, 1.0);

$secondary_fixed_dim: rgba(198, 197, 208, 1.0);

$shadow: rgba(0, 0, 0, 1.0);

$source_color: rgba(166, 182, 255, 1.0);

$surface: color.adjust($surface_bright, $alpha: -0.4);

$surface_bright: rgba(58, 57, 58, 1.0);

$surface_container: rgba(32, 31, 33, 1.0);

$surface_container_high: rgba(42, 42, 43, 1.0);

$surface_container_highest: rgba(53, 52, 54, 1.0);

$surface_container_low: rgba(28, 27, 29, 1.0);

$surface_container_lowest: rgba(14, 14, 15, 1.0);

$surface_dim: rgba(19, 19, 21, 1.0);

$surface_tint: rgba(196, 197, 214, 1.0);

$surface_variant: rgba(71, 70, 72, 1.0);

$tertiary: rgba(194, 197, 221, 1.0);

$tertiary_container: rgba(66, 70, 89, 1.0);

$tertiary_fixed: rgba(222, 225, 249, 1.0);

$tertiary_fixed_dim: rgba(194, 197, 221, 1.0);


/* general color variables */
$red: #d17678;
$accent_green: #afbea2;
$tooltipBg: $background;
$fg: $on_background;
$bg: color.adjust($background, $alpha: -0.5);
$barBg: color.adjust($background, $alpha: -0.5);
$surface: color.adjust($surface_bright, $alpha: -0.4);
$overlay: color.adjust($on_surface, $alpha: -0.25);
$accent: $tertiary_container;

/* os icon colors */
$os: $on_surface_variant;

/* workspaces colors */
$primaryMon: color.mix(;
  $primary_container,
  $on_primary_container,
  70%,
$method: rgb;
);
$primaryMonHover: color.mix(;
  $primary_container,
  $on_primary_container,
  45%,
$method: rgb;
);
$secondaryMon: color.mix(;
  $tertiary_container,
  $on_tertiary_container,
  50%,
$method: rgb;
);
$secondaryMonHover: color.mix(;
  $tertiary_container,
  $on_tertiary_container,
  30%,
$method: rgb;
);
$tertiaryMon: color.mix(;
  $secondary_container,
  $on_secondary_container,
  70%,
$method: rgb;
);
$tertiaryMonHover: color.mix(;
  $secondary_container,
  $on_secondary_container,
  45%,
$method: rgb;
);
$fourthMon: color.mix($secondary, $on_secondary, 70%, $method: rgb);
$fourthMonHover: color.mix($secondary, $on_secondary, 45%, $method: rgb);

/* button colors */
$buttonEnabled: color.adjust($primary, $alpha: -0.8);
$buttonEnabledHover: color.adjust($buttonEnabled, $alpha: +0.1);

$buttonDisabled: $surface;
$buttonDisabledHover: color.adjust($buttonDisabled, $alpha: +0.39);

/* hw circular proc colors */
$ramProc: color.mix($primary_fixed_dim, $on_primary_fixed_variant, 66%);
$cpuProc: color.mix($tertiary_fixed_dim, $on_tertiary_fixed_variant, 66%);
$procBg: $surface_dim;

/* numeric variables */
$round: 8px;
$round2: 16px;
$spacing-xs: 0.25rem;
$spacing-sm: 0.4rem;
$spacing-md: 0.75rem;
$border-width: 2px;
$scale: 0.5rem;
$font: 1.1rem;